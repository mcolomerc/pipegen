      "name": "event_type",
      "type": "string",
      "doc": "Type of event that was aggregated"
    },
    {
      "name": "user_id",
      "type": "string", 
      "doc": "User identifier"
    },
    {
      "name": "event_count",
      "type": "long",
      "doc": "Number of events in this aggregation"
    },
    {
      "name": "window_start",
      "type": {
        "type": "long",
        "logicalType": "timestamp-millis"
      },
      "doc": "Window start timestamp"
    },
    {
      "name": "window_end", 
      "type": {
        "type": "long",
        "logicalType": "timestamp-millis"
      },
      "doc": "Window end timestamp"
    }
  ]
}